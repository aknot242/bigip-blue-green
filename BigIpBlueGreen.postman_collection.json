			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "Cookie",
						"value": "BIGIPAuthCookie=7FFE6232D65012AA7D1BCAAD1C7C8320DAC72430",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"loginProviderName\": \"tmos\",\n    \"needsToken\": true\n}"
